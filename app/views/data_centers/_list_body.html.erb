<%if @data.present? %>
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>序号</th>
        <th>标题</th>
        <th>作者</th>
        <th>发布时间</th>
        <th>网站</th>
        <th>爬虫名</th>
        <th>类别</th>
      </tr>
    </thead>
    <tbody>
      <% @data.each_with_index do |x, index| %>
        <tr>
          <td><%= index %></td>
          <td>标题</td>
          <td>Paul</td>
          <td>2020-02-28</td>
          <td>www.baidu.com</td>
          <td>spider-1</td>
          <td>人工智能技术</td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
    <%#= paginate @data%>
  </div>
<%else%>
  <%= render "shared/no_data" %>
<%end%>
