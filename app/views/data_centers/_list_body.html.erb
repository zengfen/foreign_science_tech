<%if !@results.blank?%>
  <table class="table table-striped table-bordered table-hover table-vcenter">
    <thead>
      <tr>
        <th class="text-center">数据名称</th>
        <th class="text-center">更新时间</th>
        <th class="text-center">更新条数</th>
        <th class="text-center">数据条数</th>
        <th colspan="5" class="text-center">操作</th>
      </tr>
    </thead>
    <tbody class="text-center">
      <% @results.each do |inx| %>
        <%=render :partial=>"one_index_in_list",:locals=>{:inx=>inx}%>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
  </div>
<%else%>
  <%= render "shared/no_data" %>
<%end%>
