<%if !@error_tasks.blank?%>
  <table class="table table-striped table-bordered table-hover table-vcenter">
    <thead>
      <tr>
        <th class="text-center">url</th>
        <th class="text-center">retry_count</th>
        <th class="text-center">max_retry_count</th>
        <th class="text-center">操作</th>
      </tr>
    </thead>
    <tbody class="text-center">

      <% @error_tasks.each do |error_task| %>
      <tr id="error_task_<%=error_task['task_md5']%>">
       <%=render :partial=>"one_error_task_in_list",:locals=>{:error_task=>error_task}%>
      </tr>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
    <%= paginate @detail_keys%>
  </div>
<%else%>
   <%= render "shared/no_data" %>
<%end%>