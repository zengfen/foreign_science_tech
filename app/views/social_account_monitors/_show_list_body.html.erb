<%if !@accounts.blank?%>
  <table class="table table-striped table-bordered table-hover table-vcenter">
    <thead>
      <tr>
        <th class="text-center">名称</th>
        <th class="text-center">更新时间</th>
        <th class="text-center">操作</th>
      </tr>
    </thead>
    <tbody class="text-center">
      <% @accounts.each do |x| %>
        <tr>
          <td class="text-center"><%= x[0]%></td>
          <td class="text-center"><%= x[1]%></td>
          <td class="pos-r">
            <a href="##">
              <i class="icon_data_analysis icon-more"></i>
            </a>
            <ul class="more-ul dn">
              <li>
                <a href="/social_account_monitors/<%= params[:id] %>/delete_account?account=<%= x[0] %>" >列表</a>
              </li>
            </ul>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
  </div>
<%else%>
  <%= render "shared/no_data" %>
<%end%>
