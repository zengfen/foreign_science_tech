<%if !@social_accounts.blank?%>
  <table class="table table-striped table-bordered table-hover table-vcenter">
    <thead>
      <tr>
        <th class="text-center">账号名称</th>
        <th class="text-center">账号</th>
        <th class="text-center">账号性质</th>
        <th class="text-center">账号类型</th>
        <th class="text-center">账号状态</th>
        
      </tr>
    </thead>
    <tbody class="text-center">

      <% @social_accounts.each do |social_account| %>
       <%=render :partial=>"one_data_in_list",:locals=>{:social_account=>social_account}%>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
    <%= paginate @social_accounts%>
  </div>
<%else%>
   <%= render "shared/no_data" %>
<%end%>