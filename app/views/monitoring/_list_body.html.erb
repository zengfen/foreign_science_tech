<%if !@nodes.blank?%>
  <table class="table table-striped table-bordered table-hover table-vcenter">
    <thead>
      <tr>
        <th class="text-center">节点名</th>
        <th class="text-center">IP地址</th>
        <th class="text-center">规则</th>
        <th class="text-center">CPU使用率</th>
        <th class="text-center">JVM内存使用率</th>
        <th class="text-center">硬盘使用率</th>
        <th class="text-center">线程数</th>
        <th class="text-center">HTTP连接数</th>
        <!--<th class="text-center">创建时间</th>
        <th colspan="5" class="text-center">操作</th> -->
      </tr>
    </thead>
    <tbody class="text-center">
      <% @nodes["nodes"].each do |k,v| %>
        <%=render :partial=>"one_node_in_list",:locals=>{:v=>v}%>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
    <%= paginate @spiders%>
  </div>
<%else%>
  <%= render "shared/no_data" %>
<%end%>