<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">

    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>执行命令</h5>
        </div>
        <div id="result_release">
          <div class="row">
            <div class="col-lg-12">
              <div class="ibox float-e-margins">
                <div class="ibox-content">
                  <!-- 添加form -->
                  <form method="post" action="/commands/create_supervisor" class="form-horizontal">

                    <div class="form-group"><label class="col-sm-2 control-label">选择</label>

                      <div class="col-sm-10">
                        <select class="form-control m-b" name="command">
                          <% @commands.each do |k, v| %>
                            <option value="<%= k %>"><%= v %></option>
                          <% end %>
                        </select>
                      </div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                      <div class="col-sm-4 col-sm-offset-2">
                        <button class="btn btn-primary" type="submit">执行</button>
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>

